    //Make the text gold if it is the selected one
    if menu_choice = i
        menu[i] = string_lower(menu[i])

    //Otherwise, make the text white
    else
        menu[i] = string_upper(menu[i])

    //Draw the text
    draw_text(view_xview[0]+192,view_yview[0]+88+i*16,menu[i])


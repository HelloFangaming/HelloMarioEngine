    }

}

//If Mario leaves while the flippers are open
else if not collision_rectangle(x,y-1,x+63,y+19,obj_mario,0,0)
and ready = 2
{

    //Close them
    ready = 3

    //Animate the closing
    alarm[0] = 2
    alarm[2] = 4


    speed = 1
    direction = 180
    type = 2
}
else if place_meeting(x,y,obj_clockwiseb)
{
    speed = 1
    direction = 180
    type = 1
}
else if place_meeting(x,y,obj_cclockwiseb)
{
    speed = 1
    direction = 0
    type = 2
}

//Make rope
rope = instance_create(x,y+16,obj_movingrope_rope)

//Make sounds
alarm[0] = 15
</string>

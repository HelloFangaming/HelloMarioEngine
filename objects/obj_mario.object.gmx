    }

    //Make sure Mario is not climbing
    if state &gt; 2
        state = 2

}

//Prevents Mario from going too high
if y &lt; -96
    y = -96

//If he falls out of the room
else if bbox_top &gt; room_height
{

    //Give crown Mario one last chance to survive
    if global.powerup = cs_crown
    and doublejump != -1
    {

    //Handles powerup specific projectiles, tail spinning, and statue morphing
    if keyboard_check_pressed(vk_control)
    and obj_levelmanager.barrier = 0
        alarm[10] = 1

}

//If Mario is climbing or wall climbing
else
{

    //Disable ducking

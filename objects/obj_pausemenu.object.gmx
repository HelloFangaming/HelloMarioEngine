        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw the pause menu

//Draw the screenshot if it exists
if background_exists(back)
{
    draw_enable_alphablend(0)
    draw_background(back,view_xview[0],view_yview[0])
    draw_enable_alphablend(1)
}

//Center the text
draw_set_halign(fa_center)

//Set the aplha
draw_set_alpha(0.7)

//Set the font
draw_set_font(global.font)

//Darken the screen
draw_set_color(c_black)
draw_rectangle(view_xview[0],view_yview[0],view_xview[0]+view_wview[0],view_yview[0]+view_hview[0],0)
draw_set_color(c_white)

//Draw the box
draw_sprite_ext(spr_pause,1,view_xview[0]+192,view_yview[0]+108,1,1,0,c_white,0.7)

//Iterate over the menu items
for(var i = 0; i &lt; 3; i++)
{

    //Make the text gold if it is the selected one
    if menu_choice = i
        menu[i] = string_lower(menu[i])

    //Otherwise, make the text white
    else
        menu[i] = string_upper(menu[i])

    //Draw the text
    draw_text(view_xview[0]+192,view_yview[0]+88+i*16,menu[i])

}

//Reset text alignment
draw_set_halign(fa_left)

//Reset alpha
draw_set_alpha(1)
</string>
